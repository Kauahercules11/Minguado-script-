local targetPlaceId = 7549229959
local teleportPosition = Vector3.new(-44.9, 3.1, -104.2)

-- Notificação de boas-vindas (só uma vez)
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "BOAAA", 
    Text = "Boas vindas ao Round 6 hack!", 
    Icon = "rbxassetid://0" -- ⚠️ Substitua pelo ID de uma imagem válida!
})

-- Verifica se estamos no lugar correto
if game.PlaceId == targetPlaceId then
    local player = game:GetService("Players").LocalPlayer

    -- Loop infinito para teleportar a cada 1 segundo
    while true do
        if player.Character then
            local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                humanoidRootPart.CFrame = CFrame.new(teleportPosition)
                print("Teleportado para:", teleportPosition)
            end
        end
        task.wait(1) -- Espera 1 segundo antes de repetir
    end
else
    print("Este não é o lugar alvo (PlaceId incorreto).")
end
