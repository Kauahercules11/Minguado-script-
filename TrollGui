    local loopTPEnabled = false
    local loopTPConnection = nil
    local highlights = {}
    local minimizeKey = "R"
    
    -- >>> NOVAS VARIÁVEIS: Freeze Mode e Egor Mode
    local freezeModeEnabled = false
    local egorModeEnabled = false
    local originalWalkSpeed = 16
    local originalAnimations = {}
    local egorSpeedMultiplier = 3.5
    local humanoidMoveConnection = nil
    local humanoid = nil
    local animator = nil
    local walkAnimationTrack = nil
    local idleAnimationTrack = nil
    local walkAnimationId = "rbxassetid://3760534309" -- Animação de andar padrão do Roblox
    local idleAnimationId = "rbxassetid://180435571" -- Animação de idle padrão do Roblox
    local egorModeConnection = nil
    local freezeModeConnection = nil
    
    -- >>> NOVA VARIÁVEL: distância de TP (studs)
    local tpDistanceStuds = 2
    
    -- >>> NOVAS VARIÁVEIS: Click TP e CFrame Fly
    local clickTpEnabled = false
    local cframeFlyEnabled = false
    local clickTpConnection = nil
    local cframeFlyConnection = nil
    
    -- =======================
    -- FUNÇÕES
    -- =======================
    local function createESP(targetPlayer)
        if not targetPlayer.Character then return end
        if highlights[targetPlayer] then return end
        
        local highlight = Instance.new("Highlight")
        highlight.Adornee = targetPlayer.Character
        highlight.FillColor = Color3.fromRGB(0,255,0)
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        highlight.Parent = workspace
        
        highlights[targetPlayer] = highlight
    end
    
    local function removeESP(targetPlayer)
        if highlights[targetPlayer] then
            highlights[targetPlayer]:Destroy()
            highlights[targetPlayer] = nil
        end
    end
    
    local function clearAllESP()
        for p,h in pairs(highlights) do
            h:Destroy()
        end
        highlights = {}
    end
    
    local function updatePlayerExamples()
        local exampleText = "Exemplos: "
        local players = Players:GetPlayers()
        local count = 0
        
        for _, p in ipairs(players) do
            if p ~= player then
                count = count + 1
                exampleText = exampleText .. p.Name
                if count < 3 then
                    exampleText = exampleText .. ", "
                end
                if count >= 3 then
                    break
                end
            end
        end
        
        PlayerExamples.Text = exampleText
    end
    
    -- >>> FUNÇÃO PARA ANALISAR COORDENADAS
    local function parseCoordinates(text)
        -- Remove espaços e quebras de linha
        text = text:gsub("%s+", "")
        
        -- Verifica se está no formato X,Y,Z
        local x, y, z = text:match("^([%d%.%-]+),([%d%.%-]+),([%d%.%-]+)$")
        if x and y and z then
            return Vector3.new(tonumber(x), tonumber(y), tonumber(z))
        end
        
        -- Verifica se está no formato CFrame.new(X, Y, Z)
        local cframeMatch = text:match("CFrame%.new%(([%d%.%-]+),([%d%.%-]+),([%d%.%-]+)%)")
        if cframeMatch then
            local x, y, z = text:match("CFrame%.new%(([%d%.%-]+),([%d%.%-]+),([%d%.%-]+)%)")
            return Vector3.new(tonumber(x), tonumber(y), tonumber(z))
        end
        
        -- Verifica se está no formato {X, Y, Z}
        local vectorMatch = text:match("{%s*([%d%.%-]+)%s*,%s*([%d%.%-]+)%s*,%s*([%d%.%-]+)%s*}")
        if vectorMatch then
            local x, y, z = text:match("{%s*([%d%.%-]+)%s*,%s*([%d%.%-]+)%s*,%s*([%d%.%-]+)%s*}")
            return Vector3.new(tonumber(x), tonumber(y), tonumber(z))
        end
        
        return nil
    end
    
    -- >>> NOVAS FUNÇÕES: Freeze Mode e Egor Mode
    local function setupHumanoid()
        if humanoidMoveConnection then
            humanoidMoveConnection:Disconnect()
            humanoidMoveConnection = nil
        end
        
        if egorModeConnection then
            egorModeConnection:Disconnect()
            egorModeConnection = nil
        end
        
        if freezeModeConnection then
            freezeModeConnection:Disconnect()
            freezeModeConnection = nil
        end
        
        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
            humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            originalWalkSpeed = humanoid.WalkSpeed
            
            -- Encontrar ou criar animator
            animator = humanoid:FindFirstChildOfClass("Animator")
            if not animator then
                animator = Instance.new("Animator")
                animator.Parent = humanoid
            end
            
            -- Carregar animações
            if not walkAnimationTrack then
                local walkAnimation = Instance.new("Animation")
                walkAnimation.AnimationId = walkAnimationId
                walkAnimationTrack = animator:LoadAnimation(walkAnimation)
                walkAnimationTrack.Priority = Enum.AnimationPriority.Core
            end
            
            if not idleAnimationTrack then
                local idleAnimation = Instance.new("Animation")
                idleAnimation.AnimationId = idleAnimationId
                idleAnimationTrack = animator:LoadAnimation(idleAnimation)
                idleAnimationTrack.Priority = Enum.AnimationPriority.Core
            end
            
            -- Conectar para Freeze Mode - sistema mais robusto
            freezeModeConnection = RunService.Heartbeat:Connect(function()
                if freezeModeEnabled and humanoid then
                    -- Congela o personagem completamente
                    humanoid.WalkSpeed = 0
                    
                    -- Forçar animação baseada no movimento
                    if humanoid.MoveDirection.Magnitude > 0 then
                        -- Tentando se mover - tocar animação de andar
                        if walkAnimationTrack then
                            if not walkAnimationTrack.IsPlaying then
                                walkAnimationTrack:Play()
                            end
                            walkAnimationTrack:AdjustSpeed(1)
                        end
                        if idleAnimationTrack and idleAnimationTrack.IsPlaying then
                            idleAnimationTrack:Stop()
                        end
                    else
                        -- Parado - tocar animação de idle
                        if walkAnimationTrack and walkAnimationTrack.IsPlaying then
                            walkAnimationTrack:Stop()
                        end
                        if idleAnimationTrack and not idleAnimationTrack.IsPlaying then
                            idleAnimationTrack:Play()
                        end
                    end
                    
                    -- Prevenir qualquer movimento físico
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                        player.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
                    end
                end
            end)
            
            -- Conectar para Egor Mode - sistema mais robusto
            egorModeConnection = RunService.Heartbeat:Connect(function()
                if egorModeEnabled and humanoid and animator then
                    -- Acelerar TODAS as animações em execução
                    for _, track in pairs(animator:GetPlayingAnimationTracks()) do
                        if not originalAnimations[track] then
                            originalAnimations[track] = track.Speed
                        end
                        track:AdjustSpeed(egorSpeedMultiplier)
                    end
                    
                    -- Garantir que a animação de andar continue acelerada mesmo quando parado
                    if humanoid.MoveDirection.Magnitude > 0 then
                        if walkAnimationTrack and not walkAnimationTrack.IsPlaying then
                            walkAnimationTrack:Play()
                            walkAnimationTrack:AdjustSpeed(egorSpeedMultiplier)
                        end
                    end
                end
            end)
        end
    end
    
    local function toggleFreezeMode()
        freezeModeEnabled = not freezeModeEnabled
        
        if player.Character and humanoid then
            if freezeModeEnabled then
                -- Congela o personagem
                humanoid.WalkSpeed = 0
                
                -- Iniciar animação de idle
                if idleAnimationTrack then
                    idleAnimationTrack:Play()
                end
                
                -- Parar qualquer movimento imediatamente
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                    player.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
                end
            else
                -- Restaurar a velocidade normal
                humanoid.WalkSpeed = originalWalkSpeed
                
                -- Parar animações forçadas
                if walkAnimationTrack then
                    walkAnimationTrack:Stop()
                end
                if idleAnimationTrack then
                    idleAnimationTrack:Stop()
                end
            end
        end
        
        return freezeModeEnabled
    end
    
    local function toggleEgorMode()
        egorModeEnabled = not egorModeEnabled
        
        if player.Character and humanoid then
            if egorModeEnabled then
                -- Acelerar TODAS as animações
                if animator then
                    for _, track in pairs(animator:GetPlayingAnimationTracks()) do
                        if not originalAnimations[track] then
                            originalAnimations[track] = track.Speed
                        end
                        track:AdjustSpeed(egorSpeedMultiplier)
                    end
                end
                
                -- Forçar animação de andar acelerada
                if walkAnimationTrack then
                    if not walkAnimationTrack.IsPlaying then
                        walkAnimationTrack:Play()
                    end
                    walkAnimationTrack:AdjustSpeed(egorSpeedMultiplier)
                end
            else
                -- Restaurar velocidades normais das animações
                if animator then
                    for track, originalSpeed in pairs(originalAnimations) do
                        if track then
                            track:AdjustSpeed(originalSpeed)
                        end
                    end
                end
                
                if walkAnimationTrack then
                    walkAnimationTrack:AdjustSpeed(1)
                end
                
                originalAnimations = {}
            end
        end
        
        return egorModeEnabled
    end
    
    -- >>> NOVAS FUNÇÕES: Click TP e CFrame Fly
    local function toggleClickTp()
        clickTpEnabled = not clickTpEnabled
        
        if clickTpEnabled then
            -- Conectar ao evento de clique do mouse
            clickTpConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if not gameProcessed and input.UserInputType == Enum.UserInputType.MouseButton1 then
                    -- Obter a posição do mouse no mundo
                    local mousePos = UserInputService:GetMouseLocation()
                    local ray = camera:ViewportPointToRay(mousePos.X, mousePos.Y)
                    local direction = ray.Direction * 1000
                    local origin = ray.Origin
                    
                    -- Fazer raycast para encontrar onde o jogador clicou
                    local params = RaycastParams.new()
                    params.FilterType = Enum.RaycastFilterType.Blacklist
                    if player.Character then
                        params.FilterDescendantsInstances = {player.Character}
                    end
                    
                    local result = workspace:Raycast(origin, direction, params)
                    
                    if result and result.Position and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        -- Teleportar para a posição clicada
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(result.Position + Vector3.new(0, 3, 0))
                    end
                end
            end)
        else
            -- Desconectar o evento
            if clickTpConnection then
                clickTpConnection:Disconnect()
                clickTpConnection = nil
            end
        end
        
        return clickTpEnabled
    end
    
    local function toggleCframeFly()
        cframeFlyEnabled = not cframeFlyEnabled
        
        if cframeFlyEnabled then
            local flySpeed = 50
            local flyKeys = {
                [Enum.KeyCode.W] = Vector3.new(0, 0, -1),
                [Enum.KeyCode.S] = Vector3.new(0, 0, 1),
                [Enum.KeyCode.A] = Vector3.new(-1, 0, 0),
                [Enum.KeyCode.D] = Vector3.new(1, 0, 0),
                [Enum.KeyCode.Space] = Vector3.new(0, 1, 0),
                [Enum.KeyCode.LeftShift] = Vector3.new(0, -1, 0)
            }
            
            local velocity = Vector3.new()
            local lastTime = tick()
            
            cframeFlyConnection = RunService.Heartbeat:Connect(function()
                if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
                    return
                end
                
                local rootPart = player.Character.HumanoidRootPart
                local currentTime = tick()
                local deltaTime = currentTime - lastTime
                lastTime = currentTime
                
                -- Calcular direção do movimento baseado nas teclas pressionadas
                local moveDirection = Vector3.new()
                
                for key, direction in pairs(flyKeys) do
                    if UserInputService:IsKeyDown(key) then
                        moveDirection = moveDirection + direction
                    end
                end
                
                -- Normalizar a direção se estiver se movendo em múltiplas direções
                if moveDirection.Magnitude > 0 then
                    moveDirection = moveDirection.Unit
                end
                
                -- Aplicar aceleração e velocidade
                local targetVelocity = moveDirection * flySpeed
                velocity = velocity:Lerp(targetVelocity, 10 * deltaTime)
                
                -- Mover usando CFrame para evitar detecção de movimento pelo jogo
                if velocity.Magnitude > 0.1 then
                    local cameraCFrame = camera.CFrame
                    local moveVector = cameraCFrame:VectorToWorldSpace(velocity * deltaTime)
                    rootPart.CFrame = rootPart.CFrame + moveVector
                    
                    -- Manter a rotação original para parecer natural
                    rootPart.Velocity = Vector3.new()
                    rootPart.AssemblyLinearVelocity = Vector3.new()
                end
            end)
        else
            -- Desconectar o evento
            if cframeFlyConnection then
                cframeFlyConnection:Disconnect()
                cframeFlyConnection = nil
            end
            
            -- Restaurar física normal
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                player.Character.HumanoidRootPart.Velocity = Vector3.new()
                player.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new()
            end
        end
        
        return cframeFlyEnabled
    end
    
    -- Configurar humanoid quando o personagem spawnar
    player.CharacterAdded:Connect(function(character)
        wait(0.5) -- Esperar o humanoid ser criado
        setupHumanoid()
        
        -- Reaplicar modos se estiverem ativos
        if freezeModeEnabled then
            humanoid.WalkSpeed = 0
            if idleAnimationTrack then
                idleAnimationTrack:Play()
            end
        end
        
        if egorModeEnabled then
            if animator then
                for _, track in pairs(animator:GetPlayingAnimationTracks()) do
                    if not originalAnimations[track] then
                        originalAnimations[track] = track.Speed
                    end
                    track:AdjustSpeed(egorSpeedMultiplier)
                end
            end
        end
    end)
    
    if player.Character then
        setupHumanoid()
    end
    
    -- =======================
    -- BOTÕES E INTERAÇÕES
    -- =======================
    -- Noclip
    local NoclipButton = Instance.new("TextButton")
